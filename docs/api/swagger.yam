{
  "schemes": [
    "https",
    "http"
  ],
  "swagger": "2.0",
  "info": {
    "title": "GoToSocial Swagger documentation.",
    "contact": {
      "name": "GoToSocial Authors",
      "email": "admin@gotosocial.org"
    },
